{
    "name": "orderbird",
    "version": "1.0.0",
    "description": "Display github repo info",
    "main": "index.js",
    "scripts": {
        "start": "parcel src/index.html --open",
        "build": "parcel build --no-warnings src/index.html",
        "lint": "eslint . --ext js --report-unused-disable-directives --max-warnings 0",
        "test": "jest --watchAll --config=jest.config.js"
    },
    "jest": {
        "preset": "babel-jest",
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.22.20",
        "@babel/register": "^7.22.15",
        "@parcel/babel-preset-env": "^2.9.3",
        "@testing-library/jest-dom": "^6.1.3",
        "autoprefixer": "^10.4.2",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^22.1.0",
        "parcel": "^2.3.1",
        "postcss": "^8.4.5",
        "prettier": "3.0.3"
    }
}
